{
  "Hugo relref shortcode": {
    "prefix": "h-relref",
    "body": ["{{< relref \"${0}\" >}}"],
    "description": "Add a relative permalink to document"
  },
  "Hugo relanchor shortcode": {
    "prefix": "h-relanchor",
    "body": ["{{< relanchor \"${0}\" >}}"],
    "description": "Add a relative anchor to document"
  },
  "Hugo json2table shortcode": {
    "prefix": "h-json2table",
    "body": [
      "{{< json2table file=\"${1}\" headers=\"${2}\" columns=\"${3}\" pageLink=\"${0}\" >}}"
    ],
    "description": "Convert JSON to Table"
  },
  "Hugo ruby shortcode": {
    "prefix": "h-ruby",
    "body": ["{{< ruby \"${1}\" \"${0}\" >}}"],
    "description": "Add a ruby html tag to document"
  },
  "Hugo details shortcode": {
    "prefix": "h-details",
    "body": ["{{% details \"${0}\" %}}\n{{% /details %}}"],
    "description": "Add details"
  },
  "Hugo audio shortcode": {
    "prefix": "h-audio",
    "body": ["{{< audio src=\"${1}\" caption=\"${0}\" >}}"],
    "description": "Add audio html to document"
  },
  "Hugo video shortcode": {
    "prefix": "h-video",
    "body": ["{{< video src=\"${1}\" caption=\"${0}\" >}}"],
    "description": "Add video html to document"
  },
  "Hugo notice shortcode": {
    "prefix": "h-notice",
    "body": [
      "{{< notice ${1|warning,info,note,tip|} >}}",
      "${0}",
      "{{< /notice >}}"
    ],
    "description": "Add notice box to document"
  },
  "MkDocsAdmonition": {
    "prefix": "mkdocs-admonition",
    "body": [
      "!!! ${1|summary,info,tip,success,question,warning,failure,danger,bug,example,quote|} \"${2}\"",
      "",
      "    ${0}"
    ],
    "description": "Add admonition to document"
  },
  "Admonition": {
    "prefix": "admonition",
    "body": [
      ":::${1|note,tip,info,caution,danger|} ${2}",
      "",
      "${0}",
      "",
      ":::"
    ],
    "description": "Add admonition to document"
  },
  "Content tabs": {
    "prefix": "contenttabs",
    "body": [
      "===! \"${1}\"",
      "",
      "    ${2}",
      "",
      "=== \"${3}\"",
      "",
      "    ${0}"
    ],
    "description": "Add content tabs to document"
  },
  "Prettier ignore": {
    "prefix": "prettier-ignore",
    "body": ["<!-- prettier-ignore -->", "${0}"],
    "description": "Add prettier-ignore mark to document"
  },
  "Cite source": {
    "prefix": "cite",
    "body": ["-- <cite>${0}</cite>"],
    "description": "Add cited source to document"
  }
}
